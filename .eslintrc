{
    "parser": "babel-eslint",
    // "extends": "airbnb",
    "globals": {
        "document": true,
        "fetch": true,
        "navigator": true,
        "window": true
    },
    "rules": {
        "arrow-parens": "off",
        "comma-dangle": "off",
        "function-paren-newline": "off",
        "indent": "off",
        "react/jsx-indent": "off",
        "max-len": [
            1,
            150
        ],
        "no-param-reassign": "off",
        "react/require-default-props": "off"
    }
}
